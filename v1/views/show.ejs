<% include partials/header %>


<h5><%= campground.name %></h5>
<img src="<%= campground.image %>" alt="">
<p><%= campground.description %></p>

<% campground.comments.forEach(function(comment){ %>
    <p>
        <strong> 
            <%= comment.author %> 
        </strong>
            - <%= comment.text %> 
    </p>
<% }); %>


<% include partials/footer %>